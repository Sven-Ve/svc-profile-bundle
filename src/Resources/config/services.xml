<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
      <service id="svc_bundle.controller.bundle" class="Svc\ProfileBundle\Controller\BundleController" public="true">
        <call method="setContainer">
            <argument type="service" id="Psr\Container\ContainerInterface"/>
        </call>
        <tag name="container.service_subscriber"/>
      </service>

      <service id="svc_bundle.controller.change-mail" class="Svc\ProfileBundle\Controller\ChangeMailController" public="true">
        <call method="setContainer">
            <argument type="service" id="Psr\Container\ContainerInterface"/>
        </call>
        <tag name="container.service_subscriber"/>
      </service>

    </services>
</container>