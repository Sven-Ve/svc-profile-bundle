{
  "name": "svc/profile-bundle",
  "description": "Package to handle profiles (change password, change mail)",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
      {
          "name": "Sven Vetter",
          "email": "git.09o@svenvetter.com"
      }
  ],
  "require": {
    "php": "^7.3.0 | ^8",
    "svc/util-bundle": "^1|^2|dev-main",
    "symfony/framework-bundle": "^5.4",
    "doctrine/annotations": "^1.11",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/orm": "^2.8",
    "symfony/validator": "5.4.*|6.0.*",
    "symfony/translation": "5.4.*|6.0.*",
    "symfony/twig-bundle": "^4.3|^5|6.0.*",
    "symfony/security-bundle": "5.4.*|6.0.*",
    "symfony/form": "5.4.*|6.0.*"
  },
  "autoload": {
    "psr-4": {
      "Svc\\ProfileBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\ProfileBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "5.4.*|6.0.*",
    "symfony/browser-kit": "5.4.*|6.0.*",
    "phpstan/phpstan": "^1",
    "rector/rector": "^0.12"
},
  "scripts": {
      "test": "vendor/bin/simple-phpunit",
      "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyse src/ --level 5 -c .phpstan.neon"
  }
}
