{
  "name": "svc/profile-bundle",
  "description": "Private package to handle profiles",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
      {
          "name": "Sven Vetter",
          "email": "git.09o@svenvetter.com"
      }
  ],
  "require": {
    "php": "^7.3.0 | ^8.0.0",
    "svc/util-bundle": "dev-main",
    "symfony/framework-bundle": "^5",
    "doctrine/annotations": "^1.11",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/orm": "^2.8",
    "symfony/mailer": "^5",
    "symfony/validator": "^5",
    "symfony/translation": "^5",
    "symfony/twig-bundle": "^4.3|^5.0",
    "twig/twig": "^2.12|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Svc\\ProfileBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\ProfileBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^5.2"
  },
  "repositories": [{
    "type": "composer",
    "url": "https://satis.sv-systems.com"
  }]
}
