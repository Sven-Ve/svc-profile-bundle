{
  "name": "svc/profile-bundle",
  "description": "Package to handle profiles (change password, change mail)",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
      {
          "name": "Sven Vetter",
          "email": "git.09o@svenvetter.com"
      }
  ],
  "require": {
    "php": "^8.2",
    "svc/util-bundle": "^6.1 | ^7 | ^8 | dev-main",
    "symfony/framework-bundle": "^6.4 | ^7 | ^8",
    "doctrine/doctrine-bundle": "^2.10 | ^3",
    "doctrine/orm": "^2.15 | ^3",
    "symfony/validator": "^6.4 | ^7 | ^8",
    "symfony/translation": "^6.4 | ^7 | ^8",
    "symfony/twig-bundle": "^6.4 | ^7 | ^8",
    "symfony/security-bundle": "^6.4 | ^7 | ^8",
    "symfony/form": "^6.4 | ^7 | ^8"
  },
  "autoload": {
    "psr-4": {
      "Svc\\ProfileBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\ProfileBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "symfony/browser-kit": "^6.4 | ^7 | ^8",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-doctrine": "^2.0"
},
"suggest": {
  "karser/karser-recaptcha3-bundle": "Could be used in the contact form"
},
  "scripts": {
      "test": "vendor/bin/phpunit --testdox",
      "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyze -c .phpstan.neon"
  }
}
