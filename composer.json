{
  "name": "svc/profile-bundle",
  "description": "Package to handle profiles (change password, change mail)",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
      {
          "name": "Sven Vetter",
          "email": "git.09o@svenvetter.com"
      }
  ],
  "require": {
    "php": "^8.2",
    "svc/util-bundle": "^5|dev-main",
    "symfony/framework-bundle": "^6.3|^7",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/orm": "^2.15",
    "symfony/validator": "^6.3|^7",
    "symfony/translation": "^6.3|^7",
    "symfony/twig-bundle": "^6.3|^7",
    "symfony/security-bundle": "^6.3|^7",
    "symfony/form": "^6.3|^7",
    "symfony/yaml": "^6.3|^7"
  },
  "autoload": {
    "psr-4": {
      "Svc\\ProfileBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\ProfileBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2",
    "symfony/browser-kit": "^6.3|^7",
    "phpstan/phpstan": "^1.10"
},
  "scripts": {
      "test": "vendor/bin/phpunit --testdox",
      "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyse src/ --level 5 -c .phpstan.neon"
  }
}
