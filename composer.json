{
  "name": "svc/profile-bundle",
  "description": "Package to handle profiles (change password, change mail)",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
      {
          "name": "Sven Vetter",
          "email": "git.09o@svenvetter.com"
      }
  ],
  "require": {
    "php": "^8.0.2",
    "svc/util-bundle": "^3|^4|dev-main",
    "symfony/framework-bundle": "^6.1",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/orm": "^2.12",
    "symfony/validator": "^6.1",
    "symfony/translation": "^6.1",
    "symfony/twig-bundle": "^6.1",
    "symfony/security-bundle": "^6.1",
    "symfony/form": "^6.1",
    "symfony/yaml": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "Svc\\ProfileBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\ProfileBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^6.1",
    "symfony/browser-kit": "^6.1",
    "phpstan/phpstan": "^1.8"
},
  "scripts": {
      "test": "vendor/bin/simple-phpunit",
      "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyse src/ --level 5 -c .phpstan.neon"
  }
}
